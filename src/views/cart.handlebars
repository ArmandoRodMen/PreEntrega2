<h2>Los productos del carrito {{idCart}} son:</h2>

{{#each products}}
<div>
    <h3>{{product.title}}</h3>
    <p>Descripción: {{product.description}}</p>
    <p>Precio: {{product.price}}</p>
    <p>Stock: {{product.stock}}</p>
    <p>Categoría: {{product.category}}</p>
    <img src="{{product.thumbnail}}" width="100" alt="{{product.description}}">
    <p>Cantidad: {{quantity}}</p>
</div>
{{/each}}
